<%-include('navbar.ejs')%>


<link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css">
<!-- jQuery -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<!-- DataTables JS -->
<script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>

<div class="container">
    <div class="row">
        <div class="col-md-12 text-center">
            <div class="">
                <h3 class="heading mb-5">Teacher Panel
                   
                </h3>
               
            </div>
          
        </div>
    </div>
</div>

<div class="body flex-grow-1 px-3">
    <div class="container-lg">
     
  
      <div class="row">
        <div class="col-sm-6 col-lg-4">
          <div class="card mb-4 text-white bg-primary ">
            <div class="card-body pb-0 d-flex justify-content-between align-items-start">
              <div>
                <div class="fs-4 fw-semibold">Total Teacher </div>
                <span class="fs-2"><%= coun %></span>
              </div>
              <div class="dropdown">
                <button class="btn btn-transparent text-white p-0" type="button" data-coreui-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <svg class="icon">
                    <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-options"></use>
                  </svg>
                </button>
                <div class="dropdown-menu dropdown-menu-end"><a class="dropdown-item" href="#">Action</a><a class="dropdown-item" href="#">Another action</a><a class="dropdown-item" href="#">Something else here</a></div>
              </div>
            </div>
            <div class="c-chart-wrapper mt-3 mx-3" style="height:70px;">
              <canvas class="chart" id="card-chart1" height="70"></canvas>
            </div>
          </div>
        </div>
        <!-- /.col-->
        <div class="col-sm-6 col-lg-4">
          <div class="card mb-4 text-white bg-warning">
            <div class="card-body pb-0 d-flex justify-content-between align-items-start">
              <div>
                <div class="fs-4 fw-semibold text-center">
                  <h4 style="text-align: center;">Daily Presenty</h4>
                </div>
                <!-- <span class="fs-2">599</span> -->
              </div>
            </div>
            <div class="c-chart-wrapper mt- mx-" style="height:40px;">
              <canvas class="chart" id="card-chart1" height="70"></canvas>
            </div>
            <div class="row mt-3 ps-2 pe-2 pb-2">
              <div class="col-6">
                <div class="card bg-success text-white p-2">
                  <div class="card-body p-0">
                    <div class="fs-6 fw-semibold ">Present </div>
                    <span class="fs-5">300</span>
                  </div>
                </div>
              </div>
              <div class="col-6">
                <div class="card bg-danger text-white p-2">
                  <div class="card-body p-0">
                    <div class="fs-6 fw-semibold ">Absent </div>
                    <span class="fs-5">299</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
  
        <div class="col-sm-6 col-lg-4">
          <div class="card mb-4 text-white bg-dark">
            <div class="card-body pb-0 d-flex justify-content-between align-items-start">
              <div>
                <div class="fs-4 fw-semibold">Marks Sheets</div>
                <!-- <span class="fs-2">599</span> -->
              </div>
              <div class="dropdown">
                <button class="btn btn-transparent text-white p-0" type="button" data-coreui-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  <svg class="icon">
                    <use xlink:href="vendors/@coreui/icons/svg/free.svg#cil-options"></use>
                  </svg>
                </button>
                <div class="dropdown-menu dropdown-menu-end"><a class="dropdown-item" href="#">Action</a><a class="dropdown-item" href="#">Another action</a><a class="dropdown-item" href="#">Something else here</a></div>
              </div>
            </div>
            <div class="c-chart-wrapper mt-3 mx-3" style="height:120px;">
              <canvas class="chart" id="card-chart1" height="70"></canvas>
            </div>
          </div>
        </div>
      </div>
      
      <div class="row">
        <a href="/admin/teacher_approval_list">
        <div class="col-sm-6 col-lg-4">
          <div class="card mb-4 text-white bg-secondary">
            <div class="card-body pb-0 d-flex justify-content-between align-items-start">
              <div>
                <div class="fs-4 fw-semibold text-center">
                  <h4 style="text-align: center;">Teacher Approval</h4>
                </div>
                <!-- <span class="fs-2">599</span> -->
              </div>
            </div>
            <div class="c-chart-wrapper mt- mx-" style="height:40px;">
              <canvas class="chart" id="card-chart1" height="70"></canvas>
            </div>
            <div class="row mt-3 ps-2 pe-2 pb-2">
              <div class="col-6">
                <div class="card bg-success text-white p-2">
                  <div class="card-body p-0">
                    <div class="fs-6 fw-semibold ">Approved </div>
                    <span class="fs-5">300</span>
                  </div>
                </div>
              </div>
              <div class="col-6">
                <div class="card bg-danger text-white p-2">
                  <div class="card-body p-0">
                    <div class="fs-6 fw-semibold ">Rejected </div>
                    <span class="fs-5">299</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </a>
      </div>
      
             
      </div>
    </div>

    <!-- table  -->
    <div class="container mt-5">
        <div class="row">
          <div class="heading text-center">
            <h3>Teacher List</h3>
          </div>
        </div>
      </div>

    <div class="container mt-2">
        <div class="row">
          <div class="col-md-12 ">
            <table id="myTable" class=" table table-primary bordered ">
              <thead>
                <tr>
                    <th>Action</th>
                    <th>SrNO</th>
                    <th>Name</th>
                    <th>Post</th>
                    <th>ID</th>
                    <th>Mobile NO</th>
                   
                </tr>
              </thead>
              <tbody>
                <% var i=0;
                teacher_info.map((val)=>{  
                    i++;
                %>
                <tr>
                  <td><%= i %></td>
                  <td><%= val.teacher_name %></td>
                  <td><%= val.teacher_qul %></td>
                  <td><%= val.teacher_id %></td>
                  <td><%= val.teacher_mobile %></td>
                  <td><%= val.teacher_email %></td>
               
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
      </div>


        


    <script>
        $(document).ready(function() {
          $('#myTable').DataTable();
        });
      </script>
      
      <link rel="stylesheet" href="https://cdn.datatables.net/2.1.2/css/dataTables.dataTables.css" />
      <script src="https://cdn.datatables.net/2.1.2/js/dataTables.js"></script>
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.2/css/bootstrap.min.css" integrity="sha512-eWYL2j1PD1f/KNUzDGb457L/xQ5n7d5T7njfGJu/+jPAmRU8ZD7Qjv6XmTiWdS2tJcO+/EexTK+e3TB8D6YQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />

